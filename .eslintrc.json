{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "rules/prop-types": "off",
        "global-require": "off",
        "react/style-prop-object": "off",
        "react/prop-types": "off",
        "import/no-dynamic-require": "off"
    }
}
